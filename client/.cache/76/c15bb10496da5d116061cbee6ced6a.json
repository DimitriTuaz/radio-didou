h: 83%;\n}\n\n.title {\n  font-weight: bold;\n  color: white;\n  font-size:600%;\n}\n\n.sub-title {\n  font-weight: bold;\n  color: white;\n  font-size:200%;\n}\n\n.text {\n  color: white;\n}\n\n.current-listeners-container {\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  margin-top: 15px;\n}\n\n#currentListeners {\n  color: white;\n  font-size:100%;\n}\n\n.jingles-container {\n  justify-content: center;\n  align-items: center;\n}\n\n.player-jingles-container {\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n}\n\n.jingles-name {\n  color: white;\n  font-size:180%;\n}\n\n.jingles-button {\n  display: inline-block;\n  height: 150px;\n  width: 150px;\n  background-color: Transparent;\n  border: 2px solid white;\n  border-radius: 20px;\n  padding: 10px;\n  margin: 5px;\n  vertical-align: top;\n  outline:0;\n  cursor: pointer;\n}\n\n.jingles-button:hover {\n  display: inline-block;\n  height: 150px;\n  width: 150px;\n  background-color: rgba(255, 255, 255, 0.3);\n  border: 2px solid white;\n  border-radius: 20px;\n  padding: 10px;\n  margin: 5px;\n  vertical-align: top;\n  outline:0;\n  cursor: pointer;\n}\n\n.jingles-button:active {\n  display: inline-block;\n  height: 150px;\n  width: 150px;\n  background-color: rgba(0, 0, 0, 0.2);\n  border: 2px solid white;\n  border-radius: 20px;\n  padding: 10px;\n  margin: 5px;\n  vertical-align: top;\n  outline:0;\n  cursor: pointer;\n}\n\n\n\n"},"lineCount":null}},"error":null,"hash":"4f675b49d381aa7de8b84333a4d709a8","cacheData":{"env":{}}}