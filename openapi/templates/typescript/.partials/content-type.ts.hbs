{{~#if response.content}}
  {{~#each response.content as | content |}}
    {{~#if @first}}
      {{~#if content.schema}}
        {{~#unless content.schema.items}}
          {{~#if content.schema.type}}
{{content.schema.type}}
          {{~else}}
schema.{{content.schema.title}}
          {{~/if}}
        {{~else}}
          {{~#if content.schema.items.type}}
{{content.schema.items.type}}[]
          {{~else}}
schema.{{content.schema.items.title}}[]
          {{~/if}}
        {{~/unless}}
      {{/if}}
    {{~/if}}
  {{~/each}}
{{~else}}
any
{{~/if}}
