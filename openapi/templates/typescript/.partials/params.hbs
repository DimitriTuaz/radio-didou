{{~#if path.requestBody}}
  {{~#each path.requestBody.content as | content |}}
    {{~#if @first}}
{{>paramName object=content~}}: {{>type object=content~}},
    {{~/if}}
  {{~/each}}
{{~/if}}
{{~#each path.parameters as | parameter |}}
{{parameter.name}}{{{paramRequired parameter}}}: {{>type object=parameter~}},
{{~/each}}
